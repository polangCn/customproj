spring:
  profiles:
    active: {{ spring_profile }}
dubbo:
  #provider:
  #  timeout: 10000
  scan:
    base-packages: com.jx.card.account.service
  protocol:
    name: dubbo
    port: 8206
  registry:
    address: {{ nacos_servers.split() | map('regex_replace', '^(.*)$', 'spring-cloud://\\1') | list | join(';') }}
  application:
  #qos-enable: true
  cloud:
    subscribed-services: account-service
